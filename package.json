{
  "name": "cat-dash",
  "repository": "Cat-Dash",
  "version": "0.1.0",
  "title": "Cat Dash",
  "description": "A cat sitting on a mop flying in cites. Switch tracks to avoid obstacles.",
  "keywords": [
    "Princeton",
    "graphics",
    "three.js",
    "COS 426",
    "ivy league",
    "cat",
    "game",
    "flying",
    "avoid obstacles"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development --output-public-path \"\"",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "npm run prebuild && webpack --mode production --output-public-path \"\"",
    "format": "prettier --write '*.js' 'src/**/*.js'",
    "deploy": "npm run prebuild && webpack --mode production && gh-pages -d build"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "three": "^0.115.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.2.1",
    "prettier": "^2.0.4",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
